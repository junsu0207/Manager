		<typeAlias type="emp.bean.EmpDTO" alias="empDTO"/>
		<typeAlias type="member.bean.MemberDTO" alias="memberDTO"/>
		<typeAlias type="member.bean.MemberDetailDTO" alias="memberDetailDTO"/>
